<table>
    <thead>
        <tr>
            <th>Date</th>
            <th>Activity</th>
            <th>Title / Route</th>
            <th>Log</th>
            <th>Time</th>
            <th>Distance</th>
        </tr>
    </thead>
    <tbody>
    {% for session in sessions %}
        <tr>
            <td>{{ session.getDate.format('Y-m-d') }}</td>
            <td>{{ session.getActivity }}</td>
            <td><a href="{{ path("session_edit", {'session_id': session.getId}) }}">{{ session.getTitle }}</a></td>
            <td>{{ session.getLog }}</td>
            <td>{{ session.getTime }}</td>
            <td>{{ session.getDistance }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
    
